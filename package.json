{
  "name": "twitch-youtube-music-bot-multi",
  "version": "2.0.0",
  "description": "Multi-user Twitch bot for YouTube Music song requests",
  "type": "module",
  "scripts": {
    "start": "tsx src/bot/index.ts",
    "dev": "tsx watch src/bot/index.ts",
    "web": "tsx src/web/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "deploy": "concurrently 'npm run web' 'npm run start'"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@twurple/auth": "^7.3.0",
    "@twurple/chat": "^7.3.0",
    "@types/node": "^22.15.18",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "googleapis": "^156.0.0",
    "open": "^10.2.0",
    "rxjs": "^7.8.2",
    "string-similarity": "^4.0.4",
    "tsx": "^4.7.0",
    "ytmusic-api": "^5.3.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "prisma": "^6.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
